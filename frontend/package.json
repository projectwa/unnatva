{
  "name": "unnatva-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:carousel": "vite build --config vite.carousel.config.js",
    "build:counters": "vite build --config vite.counters.config.js",
    "build:media-gallery": "vite build --config vite.media-gallery.config.js",
    "build:components": "npm run build:carousel && npm run build:counters && npm run build:media-gallery",
    "build:all": "npm run build:components && npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8"
  }
}
