{
  "name": "unnatva-website",
  "version": "1.0.0",
  "description": "Unnatva Foundation Website",
  "scripts": {
    "build:css": "sass assets/scss/custom.scss assets/css/custom.css --style=expanded",
    "build:css:prod": "sass assets/scss/custom.scss assets/css/custom.css --style=compressed --no-source-map",
    "watch:css": "sass --watch assets/scss/custom.scss:assets/css/custom.css --style=expanded",
    "build": "npm run build:css:prod"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8"
  },
  "devDependencies": {
    "sass": "^1.93.3"
  }
}
